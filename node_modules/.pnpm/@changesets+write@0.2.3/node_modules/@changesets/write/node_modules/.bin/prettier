#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../prettier@2.8.4/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../../prettier@2.8.4/node_modules/prettier/bin-prettier.js" "$@"
fi
