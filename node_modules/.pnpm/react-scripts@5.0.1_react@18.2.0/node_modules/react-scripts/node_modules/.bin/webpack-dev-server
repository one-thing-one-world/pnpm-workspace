#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack-dev-server@4.11.1_webpack@5.75.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../webpack-dev-server@4.11.1_webpack@5.75.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
