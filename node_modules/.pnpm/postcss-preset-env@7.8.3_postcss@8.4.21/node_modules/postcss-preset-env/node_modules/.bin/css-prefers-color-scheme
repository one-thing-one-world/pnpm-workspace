#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../css-prefers-color-scheme@6.0.3_postcss@8.4.21/node_modules/css-prefers-color-scheme/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../../../../css-prefers-color-scheme@6.0.3_postcss@8.4.21/node_modules/css-prefers-color-scheme/dist/cli.cjs" "$@"
fi
