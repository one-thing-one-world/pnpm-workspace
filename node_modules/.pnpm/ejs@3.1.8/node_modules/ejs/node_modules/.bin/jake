#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jake@10.8.5/node_modules/jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../jake@10.8.5/node_modules/jake/bin/cli.js" "$@"
fi
